{
  "name": "spotify-related",
  "version": "1.0.0",
  "description": "A visulaization of related artists",
  "main": "app.js",
  "author": "Tyler Crosse",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node compiled/server.js",
    "dev": "node entry.js",
    "clean": "rimraf dist && rimraf compiled",
    "build": "npm run clean && npm run build:node && npm run build:dist",
    "build:node": "cross-env NODE_ENV=production npm run babel -- ./server -d ./compiled",
    "build:dist": "cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "postinstall": "npm run build",
    "babel": "babel --presets es2015,stage-0 --ignore node_modules,public",
    "lint": "eslint ."
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "d3": "^4.2.7",
    "eventemitter3": "^2.0.2",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^1.6.1",
    "json-loader": "^0.5.3",
    "node-sass": "^3.10.1",
    "precss": "^1.4.0",
    "querystring": "^0.2.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.75.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1"
  }
}
